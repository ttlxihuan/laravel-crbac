{
    "name": "ttlphp/laravel-crbac",
    "type": "library",
    "description": "基于 laravel5.1 + 权限管理，一键安装",
    "keywords": ["rbac", "laravel"],
    "homepage": "https://github.com/ttlxihuan/laravel-crbac",
    "license": "MIT",
    "authors": [
        {
            "name": "ttl",
            "email": "ttlxihuan@163.com",
            "homepage": "",
            "role": "PHP开发"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.0.*"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Crbac\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "scripts": {
        "post-package-install": [
            "php artisan crbac:table"
        ]
    }
}
